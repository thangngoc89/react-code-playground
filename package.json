{
  "name": "code-playground",
  "version": "0.1.2",
  "description": "A component for rendering HTML, CSS, Javascript code with editable source and live preview.",
  "main": "lib/index.js",
  "scripts": {
    "prebabelify": "rimraf lib",
    "babelify": "NODE_ENV=lib babel src --out-dir lib",
    "postbabelify": "DEBUG=playground:* babel-node bin/concatStyleSheet",
    "prebabelify:webpack": "rimraf webpack",
    "babelify:webpack": "babel src --out-dir webpack",
    "start": "DEBUG=playground:* babel-node bin/develop",
    "build": "DEBUG=playground:* NODE_ENV=production babel-node bin/build",
    "test": "mocha",
    "lint": "eslint .",
    "deploy": "gh-pages --repo https://$GH_TOKEN@github.com/thangngoc89/react-playground-component.git -d dist",
    "prepublish": "npm run babelify && npm run babelify:webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thangngoc89/react-playground-component.git"
  },
  "keywords": [
    "playground",
    "react"
  ],
  "author": "Khoa Nguyen <hi@khoanguyen.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thangngoc89/react-playground-component/issues"
  },
  "homepage": "https://github.com/thangngoc89/react-playground-component#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "5.0.0-beta8",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-plugin-webpack-loaders": "^0.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fs-extra": "^0.26.5",
    "gh-pages": "^0.9.0",
    "html-webpack-plugin": "^2.7.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "react-dom": "^0.14.7",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-configurator": "^0.3.1",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "react": "^0.14.7",
    "react-codemirror": "^0.2.5",
    "react-redux": "^4.1.2",
    "redux": "^3.1.4",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^1.0.3"
  }
}
