{
  "presets": ["es2015", "react", "stage-1"],
  "plugins": ["transform-runtime"],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    },
    "lib": {
      "plugins": [
        [
          "babel-plugin-webpack-loaders",
          {
            "config": "./build/webpack.config.lib.js",
            "verbose": false,
          }
        ]
      ]
    }
  }
}
